{
  "name": "lighthouse-dashboard",
  "version": "0.0.1",
  "description": "Analytics dashboard for Laravel Lighthouse GraphQL Server.",
  "author": "Robson Tenorio",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn install && yarn development --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.2.3",
    "@inertiajs/inertia-vue": "^0.2.2",
    "apexcharts": "^3.20.2",
    "axios": "^0.20.0",
    "highcharts": "^8.2.0",
    "highcharts-vue": "^1.3.5",
    "lodash": "^4.17.20",
    "normalize-text": "^2.3.1",
    "vue-apexcharts": "^1.6.0",
    "vue-moment": "^4.1.0",
    "vue-text-highlight": "^2.0.10",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.3.10",
    "vuetify-loader": "^1.6.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.5",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}